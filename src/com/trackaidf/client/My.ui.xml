<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<m:MaterialHeader ui:field="header">
			<m:MaterialNavBar height="90px" ui:field="navBar">
				<m:MaterialImage paddingLeft="300"
					url="https://lh3.googleusercontent.com/jAQwD4upABQbFPydEH0CCBvcj_IGutXPK7kmVoXHuDbsRhtf53hjvcCvWsMdMRAZl758VA=s85"></m:MaterialImage>

				<m:MaterialNavBrand ui:field="nb"
					fontSize="3.2em" text="   WELCOME TO TRACK-AID WEB PORTAL"
					paddingLeft="10" />
			</m:MaterialNavBar>
		</m:MaterialHeader>
		<m:MaterialRow></m:MaterialRow>
		<m:MaterialRow></m:MaterialRow>
		<m:MaterialRow>
			<m:MaterialColumn grid="s2" offset="s1">

				<m:MaterialCard ui:field="mc1">
					<m:MaterialCardImage ui:field="mci1"
						addStyleNames="z-depth-1">
						<m:MaterialImage ui:field="mi1"
							url="https://edmontonexpocentre.com/wp-content/uploads/2015/06/PlanEventIcon_300x300.png" />
					</m:MaterialCardImage>
					<m:MaterialCardAction ui:field="mca1">
						<m:MaterialLink text="Click to plan!"
							ui:field="ml1" />
					</m:MaterialCardAction>
				</m:MaterialCard>
			</m:MaterialColumn>

			<m:MaterialColumn grid="s2">
				<m:MaterialCard>
					<m:MaterialCardImage>
						<m:MaterialImage
							url="http://www.scacog.org/portals/9/feedback.png" />
					</m:MaterialCardImage>
					<m:MaterialCardAction>
						<m:MaterialLink ui:field="ml2" text="Design form" />
					</m:MaterialCardAction>
				</m:MaterialCard>
			</m:MaterialColumn>


			<m:MaterialColumn grid="s3">
				<m:MaterialCard>
					<m:MaterialCardImage>
						<m:MaterialImage
							url="https://blacksaildivision.com/wp-content/uploads/2015/03/centos-users-and-groups-624x390.jpg" />
					</m:MaterialCardImage>
					<m:MaterialCardAction>
						<m:MaterialLink ui:field="ml3" text="Register VolunteeR!" />
					</m:MaterialCardAction>
				</m:MaterialCard>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s2">
				<m:MaterialCard>
					<m:MaterialCardImage>
						<m:MaterialImage
							url="https://ubiq.co/static/images/marketing/tour/business-report-icon.png" />
					</m:MaterialCardImage>
					<m:MaterialCardAction>
						<m:MaterialLink ui:field="ml4" text="View Report" />
					</m:MaterialCardAction>
				</m:MaterialCard>
			</m:MaterialColumn>
		</m:MaterialRow>

	</g:HTMLPanel>
</ui:UiBinder> 