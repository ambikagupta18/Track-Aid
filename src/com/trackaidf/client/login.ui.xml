<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:d="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		
<d:MaterialHeader ui:field="header">
			<d:MaterialNavBar height="90px" ui:field="navBar">
				<d:MaterialImage paddingLeft="300"
					url="https://lh3.googleusercontent.com/jAQwD4upABQbFPydEH0CCBvcj_IGutXPK7kmVoXHuDbsRhtf53hjvcCvWsMdMRAZl758VA=s85"></d:MaterialImage>

				<d:MaterialNavBrand ui:field="nb"
					fontSize="3.2em" text="   WELCOME TO TRACK-AID "
					paddingLeft="10" />
			</d:MaterialNavBar>
		</d:MaterialHeader>
		<d:MaterialRow></d:MaterialRow>
		<d:MaterialRow></d:MaterialRow>
	
	<d:MaterialRow><d:MaterialTextBox ui:field="t1" label="Username" iconType="ACCOUNT_CIRCLE" iconSize="MEDIUM" grid="s4" offset="s4"></d:MaterialTextBox>
	</d:MaterialRow>
	<d:MaterialRow>
	<d:MaterialTextBox ui:field="t2" label="Password" iconType="LOCK" grid="s4" offset="s4"></d:MaterialTextBox></d:MaterialRow>	
	<d:MaterialRow >
	<d:MaterialButton ui:field="b1" text="Login" grid="s4" offset="s4"></d:MaterialButton>
	</d:MaterialRow>
	<d:MaterialRow>
	<d:MaterialButton ui:field="b2" text="Sign in as Volunteer" grid="s4" offset="s4"></d:MaterialButton>
	</d:MaterialRow>    
	</g:HTMLPanel>
</ui:UiBinder> 